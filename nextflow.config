}

conda.enabled = true

profiles{
    standard {
        process {
            executor = 'local'
            withLabel: big_mem {
                cpus = 4
                memory = 15.GB
            }
            withLabel: small_mem {
                cpus = 2
                memory = 8.GB
            }
        }
        
    }

    cluster {
        process {
            executor = 'local'
            withLabel: big_mem {
                cpus = 32
                memory = 64.GB
            }
            withLabel: small_mem {
                cpus = 16
                memory = 32.GB
                // queue = ...
            }
        }
    }
}


process {
    echo = true

    singularity {
        enabled = true
        runOptions = "--bind $PWD"
        cacheDir = "$PWD"
    }
    
    withName:demuxem{
       conda = './conda/demuxem_py.yml'
    }
    withName:hashedDrops{
        conda = './conda/dropletutils.yml'
    }
    withName:hash_solo {
        conda = './conda/hashsolo_py.yml'
    }
    withLabel:seurat{
        conda = './conda/rseurat.yml'
    }
    withName:solo {
        conda = './conda/solo_py.yml'
    }
    withName:summary {
        conda = './conda/summary.yml'
    }
    withName:matchDonor {
        conda = './conda/donor_match.yml'
    }
    withName:demuxlet{
        conda = 'bioconda::popscle'
    }
    withName:freemuxlet{
        conda = 'bioconda::popscle'
    }
    withName:vireo {
        conda = "aksarkar::vireosnp"
    }
    withName:freebayes {
        conda = 'bioconda::freebayes=1.2'
    }
    withName:scSplit {
        conda = './conda/scsplit.yml'
    }
    withName:cellSNP {
        conda = 'bioconda::cellsnp-lite'
    }
    withName:samstool{
        conda = 'bioconda::samtools'
    }
    withName:bcftools {
        conda = 'bioconda::bcftools'
    }
    withName:summary_all {
        conda = "./conda/summary.yml"
    }
    withName:souporcell{
        container = "shub://wheaton5/souporcell"
    }

}

